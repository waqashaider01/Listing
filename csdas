    <script>

        function toggleYes() {
            var yesElement = document.querySelector('.yestoggle');
            if (yesElement.textContent === 'Yes') {
                yesElement.textContent = 'No';

            } else {
                yesElement.textContent = 'Yes';
            }
        }
    </script>
    <script>
        function toggleYes() {
            var yesElement = document.querySelector('.yestogglebtn');
            if (yesElement.textContent === 'Yes') {
                yesElement.textContent = 'No';

            } else {
                yesElement.textContent = 'Yes';
            }
        }
    </script>


<script>
    $(function () {
        var inputs = $('input');
        var checked = inputs.filter(':checked').val();
        inputs.on('click', function () {

            if ($(this).val() === checked) {
                $(this).prop('checked', false);
                checked = '';

            } else {
                $(this).prop('checked', true);
                checked = $(this).val();
            }
        });

    });
</script>



